{
  "version": 2,
  "builds": [
    {
      "src": "api/analyze.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/analyze",
      "dest": "api/analyze.py"
    },
    {
      "src": "/(.*)",
      "dest": "client/$1"
    }
  ],
  "functions": {
    "api/analyze.py": {
      "maxDuration": 30
    }
  }
}
