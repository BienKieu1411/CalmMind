{
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.11" }
    },
    { "src": "client/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/analyze", "dest": "/api/main.py" },
    { "src": "/(.*)", "dest": "/client/$1" }
  ]
}