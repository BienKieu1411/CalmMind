{
  "version": 2,
  "builds": [
    {
      "src": "server/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "client/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/analyze",
      "methods": ["POST"],
      "dest": "/server/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
